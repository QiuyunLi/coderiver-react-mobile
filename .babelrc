{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "useBuiltIns": "usage"
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
      ["react-css-modules", {
        "exclude": "node_modules"
      }],
      ["import", { "libraryName": "antd-mobile", "style": "css" }]
    ]
}
